@model IEnumerable<LibraryMVC.Dtos.Authors.AuthorDto>

@{
    ViewData["Title"] = "Authors";
}

<h2>Authors</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var author in Model)
        {
            <tr>
                <td>@author.FirstName</td>
                <td>@author.LastName</td>
            </tr>
        }
    </tbody>
</table>
